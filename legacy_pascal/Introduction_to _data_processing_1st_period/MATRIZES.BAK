Program Matrizes;
uses crt;
var
vetor: array [1..2] of integer;
matriz: array [1..2, 1..3] of real;
I,j: integer;
begin
for I:=1 to 2 do
readln(vetor [I]);
for I:=1 to 2 do
writeln(vetor [I]);
for I:=1 to 2 do
for j:=1 to 3 do
readln(matriz[I,j]);
for I:=1 to 2 do
for j:=1 to 3 do
writeln(matriz[I,j]);
delay (6000);
end.