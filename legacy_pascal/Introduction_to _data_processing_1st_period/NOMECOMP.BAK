program Nome_Completo;
uses crt;
var
nome:string;
sobrenome:string;
nometodo:string;
begin
writeln('Qual o seu primeiro nome?');
readln(nome);
writeln('Qual o(s) seu(s) sobrenome(s)?');
readln(sobrenome);
nometodo:=nome+' '+sobrenome;
writeln('Seu nome todo ‚: ',nometodo);
writeln('Tecle <ENTER> para encerrar');
end.